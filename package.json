{
  "name": "monaco-language-turtle",
  "version": "0.0.1",
  "description": "Turtle support for Monaco Editor",
  "author": {
    "name": "Philippe Duchesne",
    "email": "pduchesne@gmail.com"
  },
  "scripts": {
    "build": "yarn clean && yarn build-tsc && yarn build-rollup",
    "build-tsc": "tsc -b .",
    "build-rollup": "rollup -c",
    "test": "jest --verbose --passWithNoTests",
    "clean": "rm -rf dist/ tsconfig.tsbuildinfo",
    "antlr4ts": "antlr4ts ./turtle.g4 -o ./src/turtle/ANTLR"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "jest": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "rollup": "^2.70.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "tsc": "^2.0.4",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.2",
    "monaco-editor": "^0.33.0"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4",
    "rdflib": "^2.2.18"
  },
  "peerDependencies": {
    "monaco-editor": "^0.33.0"
  }
}
